#!/bin/sh

set -e

case "$1" in 
    remove)
        rm -f /var/lib/comitup/comitup.json*
        rm -f /var/lib/comitup/dhcpleaseinfo
    ;;

    upgrade|deconfigure|failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \'$1\'" >&2
        exit 1
    ;;

esac

